<script>
(function(){
    var navLinks = function(){

        var items = $('#Pages li');

        items.each(function(){

            var url = this.getAttribute("data-href");

            if (url == window.location.href) {
                this.classList.add('selected');
            }

        });

    };

    window.addEventListener("load", navLinks, false);

}());
</script>
